# Ingrid Environment Variables
# Copy this file to .env and fill in your API keys

# ===========================================
# LLM Provider API Keys
# ===========================================

# Anthropic (Claude)
ANTHROPIC_API_KEY=

# Google AI (Gemini)
GOOGLE_AI_API_KEY=

# HuggingFace
HUGGINGFACE_API_KEY=

# Ollama Cloud (https://ollama.com/account)
# Used for cloud-hosted models like qwen3-vl:235b-cloud
OLLAMA_API_KEY=

# ===========================================
# Optional: Override default paths
# ===========================================
# INGRID_CONFIG_PATH=config.yaml
# INGRID_DATA_PATH=data
# INGRID_OUTPUT_PATH=output
